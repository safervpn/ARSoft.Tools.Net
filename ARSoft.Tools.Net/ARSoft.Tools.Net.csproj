<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>ARSoft.Tools.Net - C# DNS client/server and SPF Library (https://github.com/alexreinert/ARSoft.Tools.Net)</Description>
    <TargetFramework>netstandard2.1</TargetFramework>
    <NoWarn>$(NoWarn);CS1591</NoWarn>
    <Version>1.5.0</Version>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackOnBuild>true</PackOnBuild>
    <Authors>Alexander Reinert</Authors>
    <PackageId>ARSoft.Tools.Net</PackageId>
    <PackageVersion>2.2.11.0</PackageVersion>
    <PackageProjectUrl>https://github.com/alexreinert/ARSoft.Tools.Net</PackageProjectUrl>
    <PackageLicense>LICENSE</PackageLicense>
    <Title>SpeedTest.Net</Title>
    <LangVersion>8</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="..\LICENSE">
      <Link>LICENSE</Link>
    </Content>
    <Content Include="..\NOTICE">
      <Link>NOTICE</Link>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="BouncyCastle.NetCore" Version="1.8.8" />
  </ItemGroup>
</Project>
